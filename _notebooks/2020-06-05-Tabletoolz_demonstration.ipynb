   "source": [
    "# Mutate\n",
    "df = (models \n",
    "      >> tbs.to_statement \n",
    "      >> tbs.group_by([T.brand_id]) \n",
    "      >> tbs.mutate(model_base_prices = Mean(T.model_base_price)) \n",
    "      >> tbs.select([T.brand_id,'model_base_prices'])\n",
    "      >> tbs.head(num=5) \n",
    "      >> tbs.to_pandas(engine)); df"
   ]
  },
